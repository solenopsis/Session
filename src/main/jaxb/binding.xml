<jaxws:bindings xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
				xmlns:xs="http://www.w3.org/2001/XMLSchema"
				xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
				xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                version="2.1">
    <jaxb:globalBindings generateElementProperty="false" typesafeEnumMaxMembers="2048"/>

    <jaxb:bindings node="//xs:simpleType[@name='StatusCode']" typesafeEnumMaxMembers="false"/>

    <jaxb:bindings node="//xs:simpleType[@name='ExtendedErrorCode']" typesafeEnumMaxMembers="2048"/>

    <jaxb:bindings node="//xs:simpleType[@name='ExtendedCode']" typesafeEnumMaxMembers="2048"/>

    <jaxb:bindings node="//xs:simpleType[@name='FaultCode']" typesafeEnumMaxMembers="2048"/>
</jaxws:bindings>